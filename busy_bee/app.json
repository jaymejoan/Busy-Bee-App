{
  "expo": {
    "name": "busy_bee",
    "slug": "busy_bee",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/adaptive-icon.png",
    "splash": {
      "image": "./src/assets/SplashScreen.png",
      "resizeMode": "contain",
      "backgroundColor": "#fcd95c"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#fcd95c"
      }
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    }
  }
}
